<p id="notice"><%= notice %></p>

<h1>Listing Space Rols</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @space_rols.each do |space_rol| %>
      <tr>
        <td><%= space_rol.name %></td>
        <td><%= space_rol.description %></td>
        <td><%= link_to 'Show', space_rol %></td>
        <td><%= link_to 'Edit', edit_space_rol_path(space_rol) %></td>
        <td><%= link_to 'Destroy', space_rol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Space rol', new_space_rol_path %>
