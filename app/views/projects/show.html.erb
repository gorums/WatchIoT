									<label for="exampleInputName2">Name</label>
									<input type="text" class="form-control" id="exampleInputName2" placeholder="Name">
								</div>

								<button type="submit" class="btn btn-default">Add stage</button>
							</form>

							<br /><br />

							<table class="table table-striped">
								<thead>
								<tr>
									<th>Stage name</th>
									<th></th>
								</tr>
								</thead>
								<tbody>
								<tr>
									<th scope="row">Normal</th>
									<td>
										<a href="/projects/delete/1" class="list-action clear-background" title="Delete project"><i class="fa fa-trash fa-2x"></i></a>
									</td>
								</tr>
								<tr>
									<th scope="row">Warning</th>
